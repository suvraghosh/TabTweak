{
    "manifest_version": 3,
    "name": "TabTweak",
    "version": "1.0.0",
    "description": "Your own customized tab",
    "permissions": ["activeTab", "geolocation"],

    "icons": {
        "32": "/assests/images/neutrabize-32.png",
        "64": "/assests/images/neutrabize-64.png",
        "128": "/assests/images/neutrabize-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}